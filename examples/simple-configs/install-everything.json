{
  "run_list": [
    "recipe[axonops::java]",
    "recipe[axonops::cassandra]",
    "recipe[axonops::agent]"
  ],
  "axonops": {
    "agent": {
      "org": "REPLACE_WITH_YOUR_ORG_NAME",
      "org_key": "REPLACE_WITH_YOUR_ORG_KEY",
      "cluster_name": "new-cluster"
    },
    "cassandra": {
      "cluster_name": "new-cluster",
      "listen_address": "REPLACE_WITH_YOUR_SERVER_IP",
      "rpc_address": "REPLACE_WITH_YOUR_SERVER_IP",
      "seeds": "REPLACE_WITH_YOUR_SERVER_IP",
      "heap_size": "4G"
    }
  }
}