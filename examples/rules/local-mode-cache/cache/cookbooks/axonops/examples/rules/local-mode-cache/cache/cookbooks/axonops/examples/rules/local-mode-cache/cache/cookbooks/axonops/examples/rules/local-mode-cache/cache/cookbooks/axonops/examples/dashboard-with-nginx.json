{
  "run_list": [
    "recipe[axonops::dashboard]"
  ],
  "axonops": {
    "deployment_mode": "self-hosted",
    "server": {
      "enabled": true
    },
    "dashboard": {
      "enabled": true,
      "nginx_proxy": true,
      "nginx": {
        "server_name": "axonops.example.com",
        "listen_port": 80,
        "ssl_enabled": false
      }
    }
  }
}