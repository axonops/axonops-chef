# AxonOps Dashboard Configuration
# Generated by Chef - Local modifications will be overwritten

axon-dash:
  host: "<%= @listen_host %>"
  port: <%= @listen_port %>

axon-server:
  private_endpoints: "<%= @server_endpoint %>"
<% if @context_path && !@context_path.empty? -%>
  context_path: "<%= @context_path %>"
<% else %>
  context_path: ""
<% end -%>
